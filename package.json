{
  "name": "armstrong-react",
  "version": "1.0.0-alpha.17",
  "description": "Rocketmakers Armstrong library of React components",
  "main": "./dist/index.js",
  "style": "./dist/style.scss",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p source -d && node sass_concat.js",
    "init": "typings install && npm run build",
    "prepublish": "typings install && npm run build",
    "patch": "npm --no-git-tag-version version patch -m \"Upgrade to %s\"",
    "patch-publish": "npm run patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/armstrong-react.git"
  },
  "keywords": [
    "typescript",
    "React",
    "sass"
  ],
  "author": "developers@rocketmakers.com",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.14.1",
    "underscore": "^1.8.3"
  },
  "peerDependencies": {
    "react": "15.x",
    "react-dom": "15.x"
  },
  "devDependencies": {
    "import-resolve": "^0.2.6",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
  }
}
